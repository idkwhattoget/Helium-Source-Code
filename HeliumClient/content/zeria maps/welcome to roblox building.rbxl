function distLineSegmentPoint(startP, endP, point)
&#9;local x = endP - startP
&#9;local y = point - startP
&#9;local z = point - endP

&#9;if y:Dot(x) &lt;= 0 then
&#9;&#9;-- return distance to startPoint
&#9;&#9;return y.magnitude
&#9;elseif z:Dot(x) &gt;= 0 then
&#9;&#9;-- return distance to endPoint
&#9;&#9;return z.magnitude
&#9;else
&#9;&#9;-- return distance to line
&#9;&#9;x = x.unit
&#9;&#9;return (y - y:Dot(x)*x).magnitude
&#9;&#9;--(y - y:Dot(x)*x.unit).magnitude [first x needed to be unit vector, or projection got messed up]
&#9;end
end

function findClosestEdge(point)
&#9;local closestWaypoint = findClosestWaypoint(point)

&#9;-- now walk through list of neighboring edges, and calculate line-segment-to-point distances for each
&#9;local neighbors = network[closestWaypoint.Name]
&#9;local startPoint = closestWaypoint.Position

&#9;local bestDistance = nil
&#9;local bestNeighbor = nil

&#9;for c = 1, #neighbors do
&#9;&#9;local currWayPoint = wayPoints:FindFirstChild(neighbors[c])
&#9;&#9;local endPoint = currWayPoint.Position
&#9;&#9;local distToPoint = distLineSegmentPoint(startPoint, endPoint, point)

&#9;&#9;if (notTemp(currWayPoint)) then -- only check non-temporary and non-garbage waypoints
&#9;&#9;&#9;if bestDistance == nil or distToPoint &lt; bestDistance then
&#9;&#9;&#9;&#9;bestDistance = distToPoint
&#9;&#9;&#9;&#9;bestNeighbor = neighbors[c]
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end

&#9;return closestWaypoint.Name, bestNeighbor
end


