&#9;&#9;tween.Size.xOffset.Value = params.Size.X.Offset
&#9;&#9;tween.Size.yScale.Value  = params.Size.Y.Scale
&#9;&#9;tween.Size.yOffset.Value = params.Size.Y.Offset
&#9;end
&#9;if params.easing then tween.easing.Value = params.easing&#9;end
&#9;if params.overwrite then tween.overwrite.Value = params.overwrite end
&#9;if params.transformAroundCenter then tween.transformAroundCenter.Value = params.transformAroundCenter end
&#9;if params.delay then tween.delay.Value = params.delay end
&#9;tween.Parent = target
&#9;tween.Disabled = false -- go go go!
end
