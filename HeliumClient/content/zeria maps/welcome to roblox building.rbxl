function findClosestWaypoint(point)
&#9;local bestDistSq = nil
&#9;local bestWaypoint = nil
&#9;local tempDistSq
&#9;waypoints = wayPoints:GetChildren()
&#9;for k = 1, #waypoints do
&#9;&#9;tempDistSq = (waypoints[k].Position - point):Dot(waypoints[k].Position - point)
&#9;&#9;if (notTemp(waypoints[k])) then -- only check non-garbage and non-temporary waypoints
&#9;&#9;&#9;if bestWaypoint == nil or tempDistSq &lt; bestDistSq then
&#9;&#9;&#9;&#9;bestDistSq = tempDistSq
&#9;&#9;&#9;&#9;bestWaypoint = waypoints[k]
&#9;&#9;&#9;end
&#9;&#9;end
&#9;end

&#9;return bestWaypoint
end


