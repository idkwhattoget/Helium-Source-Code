&#9;if(player.Name ~= buildingArea.Player.Value) then
&#9;&#9;local popupText = &quot;This area belongs to &quot; .. buildingArea.Player.Value .. &quot;.  You don&apos;t have permission.&quot;
&#9;&#9;if(buildingArea.Player.Value == &quot;&quot;) then
&#9;&#9;&#9;popupText = &quot;This area has no owner.&quot;
&#9;&#9;end
&#9;&#9;showPopup(player, &quot;error&quot;, &quot;Can&apos;t Clear This Area&quot;, popupText)
&#9;&#9;return
&#9;end
&#9;-- This player has permission
&#9;local title = &quot;Clear Your Baseplate?&quot;
&#9;local text = &quot;Clear all your parts from your baseplate?  This cannot be undone.&quot;
&#9;local btn1Text = &quot;Clear Baseplate&quot;
