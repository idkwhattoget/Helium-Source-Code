function goToPage(n)
&#9;--print(&quot;showTutorialPage()&quot;, n)
&#9;local page = pageArray[n]
&#9;-- check if &quot;n&quot; is in bounds
&#9;if not page then
&#9;&#9;--print(n, &quot;is not a page. Doing nothing.&quot;)
&#9;&#9;return
&#9;end
&#9;hideAllPages()
&#9;wait(0.3)
&#9;--print(&quot;all pages hidden.  Showing&quot;, pageArray[n].Name)
&#9;currentPage = n
&#9;-- Save Progress
&#9;saveTutorialProgress(n)
&#9;-- show page
&#9;local pageElements = page:GetChildren()
&#9;-- hide everything
&#9;for i=1, #pageElements do
&#9;&#9;pcall(function() pageElements[i].Visible = false end)
&#9;end
&#9;-- shrink frame a little bit
&#9;local startSize = page.Size
&#9;local startPosition = page.Position
&#9;--print(&quot;startSize:&quot;, startSize.X.Scale, startSize.X.Offset, startSize.Y.Scale, startSize.Y.Offset)
&#9;--print(&quot;startPosition:&quot;, startPosition.X.Scale, startPosition.X.Offset, startPosition.Y.Scale, startPosition.Y.Offset)
&#9;--print(&quot;gotoPage: setting pre-size...&quot;)
&#9;page.Size = UDim2.new( startSize.X.Scale, startSize.X.Offset-shrinkAmount, startSize.Y.Scale, startSize.Y.Offset-shrinkAmount )
&#9;page.Position = UDim2.new( startPosition.X.Scale, -page.Size.X.Offset/2, startPosition.Y.Scale, -page.Size.Y.Offset/2 )
&#9;page.Visible = true
&#9;-- animate frame back to original size
&#9;--print(&quot;gotoPage: animating to real size...&quot;)
&#9;tween(page, tweenTimeIn, {easing=easeIn, transformAroundCenter=false, overwrite=true, Position=startPosition, Size=startSize})
&#9;wait(tweenTimeIn)
&#9;--print(&quot;gotoPage: killing tweens...&quot;)
&#9;killTweens(page)
&#9;-- show everything
&#9;--print(&quot;gotoPage: showing everything...&quot;)
&#9;for i=1, #pageElements do
&#9;&#9;pcall(function() pageElements[i].Visible = true end)
&#9;end

&#9;-- Show skip button
&#9;addSkipButton(page)

&#9;-- Animate things on the page
&#9;if     n==1 then animate1()
&#9;elseif n==2 then animate2()
&#9;elseif n==3 then animate3()
&#9;elseif n==4 then animate4()
&#9;elseif n==5 then animate5()
&#9;elseif n==6 then animate6()
&#9;elseif n==7 then animate7()
&#9;elseif n==8 then animate8()
&#9;elseif n==9 then animate9()
&#9;end
end

