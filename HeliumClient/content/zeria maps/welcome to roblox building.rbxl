&#9;mouse.mouse2.Visible = true
&#9;mouse.mouse.Visible = false
&#9;wait(0.25)
&#9;mouse.mouse2.Visible = false
&#9;mouse.mouse.Visible = true
&#9;wait(0.25)
&#9;mouse.mouse2.Visible = true
&#9;mouse.mouse.Visible = false
&#9;wait(0.5)
&#9;-- Move left-right
&#9;mouse.arrows.Visible = true
&#9;local d = 25
&#9;local t = 0.85
&#9;local p = mouse.mouse2.Position
&#9;local easing = &quot;Sine.easeInOut&quot;
&#9;tween(mouse.mouse2, t, {easing=easing, Position=UDim2.new(p.X.Scale, p.X.Offset+d, p.Y.Scale, p.Y.Offset)})
&#9;wait(t)
&#9;tween(mouse.mouse2, t, {easing=easing, Position=UDim2.new(p.X.Scale, p.X.Offset-d, p.Y.Scale, p.Y.Offset)})
&#9;wait(t)
&#9;tween(mouse.mouse2, t, {easing=easing, Position=UDim2.new(p.X.Scale, p.X.Offset+d, p.Y.Scale, p.Y.Offset)})
&#9;wait(t)
&#9;tween(mouse.mouse2, t, {easing=easing, Position=UDim2.new(p.X.Scale, p.X.Offset-d, p.Y.Scale, p.Y.Offset)})
&#9;wait(t)
&#9;tween(mouse.mouse2, t/2, {easing=easing, Position=UDim2.new(p.X.Scale, p.X.Offset, p.Y.Scale, p.Y.Offset)})

&#9;local camera = game.Workspace.CurrentCamera
&#9;local startRotation = camera.CoordinateFrame.lookVector
&#9;local timeStart = time()
&#9;local timeout = 10
&#9;-- Polling loop (because .Changed event wasn&apos;t firing when player rotates his camera.)
&#9;while true do
&#9;&#9;local currentRotation = camera.CoordinateFrame.lookVector
&#9;&#9;if math.abs(currentRotation.X - startRotation.X) &gt; 0.3 then goToPage(3) return end
&#9;&#9;if math.abs(currentRotation.Y - startRotation.Y) &gt; 0.3 then goToPage(3) return end
&#9;&#9;if math.abs(currentRotation.Z - startRotation.Z) &gt; 0.3 then goToPage(3) return end
&#9;&#9;if(time() &gt; timeStart+timeout) then goToPage(2) return end
&#9;&#9;wait(0.5)
&#9;end
end

