waitForChild(game.Workspace, &quot;BuildingAreas&quot;)
waitForChild(game, &quot;Players&quot;)

local activeParts
if(game.Workspace:FindFirstChild(&quot;ActiveParts&quot;)==nil) then
&#9;activeParts = Instance.new(&quot;Model&quot;)
&#9;activeParts.Name = &quot;ActiveParts&quot;
