function hideAllPages()
&#9;--print(&quot;hideAllPages()&quot;)
&#9;-- loop through all pages
&#9;for i=1, #pageArray do
&#9;&#9;if pageArray[i].Visible then
&#9;&#9;&#9;-- if page is visible, then hide page.
&#9;&#9;&#9;--print(pageArray[i].Name, &quot;is visible. Hiding...&quot;)
&#9;&#9;&#9;local page = pageArray[i]
&#9;&#9;&#9;local pageElements = page:GetChildren()
&#9;&#9;&#9;-- hide everything
&#9;&#9;&#9;for i=1, #pageElements do
&#9;&#9;&#9;&#9;pcall(function() pageElements[i].Visible = false end)
&#9;&#9;&#9;end
&#9;&#9;&#9;-- shrink frame a little bit
&#9;&#9;&#9;local startSize = page.Size
&#9;&#9;&#9;local startPosition = page.Position
&#9;&#9;&#9;local smallSize = 20
&#9;&#9;&#9;tween(page, tweenTimeOut, {easing=easingOut, Size=UDim2.new(page.Size.X.Scale, smallSize, page.Size.Y.Scale, smallSize), Position=UDim2.new(page.Position.X.Scale, -smallSize/2, page.Position.Y.Scale, -smallSize/2)})
&#9;&#9;&#9;wait(tweenTimeOut)
&#9;&#9;&#9;killTweens(page)
&#9;&#9;&#9;-- hide page
&#9;&#9;&#9;page.Visible = false
&#9;&#9;&#9;-- put page back to normal size.
&#9;&#9;&#9;page.Size = startSize
&#9;&#9;&#9;page.Position = startPosition
&#9;&#9;end
&#9;end
end

