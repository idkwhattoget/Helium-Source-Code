&#9;&#9;tween.Position.xOffset.Value = params.Position.X.Offset
&#9;&#9;tween.Position.yScale.Value  = params.Position.Y.Scale
&#9;&#9;tween.Position.yOffset.Value = params.Position.Y.Offset
&#9;end
&#9;if(params.Size) then
&#9;&#9;tween.Size.Value = true
