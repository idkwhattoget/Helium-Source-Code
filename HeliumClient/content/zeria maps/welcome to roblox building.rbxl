local playerGui
local popup
local btn1Connection
local btn2Connection
local btn3Connection
function hidePopup()
&#9;--print(&quot;Removing connections...&quot;)
&#9;if btn1Connection then btn1Connection:disconnect() end
&#9;if btn2Connection then btn2Connection:disconnect() end
&#9;if btn3Connection then btn3Connection:disconnect() end
&#9;if popupConnection then popupConnection:disconnect() end
&#9;popup.Delete.Disabled = false
end
function showPopup(player, style, title, text, btn1, btn1Handler, btn2, btn2Handler, btn3, btn3Handler, timeout)
&#9;--print(&quot;waiting for PlayerGui&quot;)
&#9;waitForChild(player, &quot;PlayerGui&quot;)
&#9;playerGui = player.PlayerGui
&#9;--print(&quot;waiting for popup&quot;)
&#9;waitForChild(playerGui, &quot;Popup&quot;)
&#9;popup = playerGui.Popup
&#9;--print(&quot;waiting for add, delete, title, text, btn1-3&quot;)
&#9;waitForChild(popup, &quot;Add&quot;)
&#9;waitForChild(popup, &quot;Delete&quot;)
&#9;waitForChild(popup, &quot;Title&quot;)
&#9;waitForChild(popup, &quot;Text&quot;)
&#9;waitForChild(popup, &quot;Btn1&quot;)
&#9;waitForChild(popup, &quot;Btn2&quot;)
&#9;waitForChild(popup, &quot;Btn3&quot;)
&#9;waitForChild(popup, &quot;Style&quot;)
&#9;waitForChild(popup, &quot;Timeout&quot;)
&#9;waitForChild(popup, &quot;Frame&quot;)
&#9;waitForChild(popup.Frame, &quot;Btn1&quot;)
&#9;waitForChild(popup.Frame, &quot;Btn2&quot;)
&#9;waitForChild(popup.Frame, &quot;Btn3&quot;)
&#9;waitForChild(popup.Frame.Btn1, &quot;Clicked&quot;)
&#9;waitForChild(popup.Frame.Btn2, &quot;Clicked&quot;)
&#9;waitForChild(popup.Frame.Btn3, &quot;Clicked&quot;)

&#9;-- Wait for previous popups to hide
&#9;--print(&quot;waiting for previous popups to hide...&quot;)
&#9;while (popup.Add.Disabled == false) do
&#9;&#9;popup.Add.Changed:wait()
&#9;end
&#9;-- Wait for delete script to finish (if necessary)
&#9;--print(&quot;waiting for delete script to finish...&quot;)
&#9;while not popup.Delete.Disabled do
&#9;&#9;popup.Delete.Changed:wait()
&#9;end
&#9;-- Add text
&#9;if style then popup.Style.Value = style end
&#9;if title then popup.Title.Value = title end
&#9;if text  then popup.Text.Value  = text end
&#9;if btn1  then popup.Btn1.Value  = btn1 end
&#9;if btn2  then popup.Btn2.Value  = btn2 end
&#9;if btn3  then popup.Btn3.Value  = btn3 end
&#9;if timeout then popup.Timeout.Value = timeout end
&#9;-- Connect events
&#9;btn1Connection = popup.Frame.Btn1.Clicked.Changed:connect(
&#9;&#9;function() 
&#9;&#9;&#9;if popup.Frame.Btn1.Clicked.Value then 
&#9;&#9;&#9;&#9;hidePopup()
&#9;&#9;&#9;&#9;if btn1Handler then btn1Handler() end
&#9;&#9;&#9;end
&#9;&#9;end)
&#9;btn2Connection = popup.Frame.Btn2.Clicked.Changed:connect(
&#9;&#9;function() 
&#9;&#9;&#9;if popup.Frame.Btn2.Clicked.Value then 
&#9;&#9;&#9;&#9;hidePopup()
&#9;&#9;&#9;&#9;if btn2Handler then btn2Handler() end
&#9;&#9;&#9;end
&#9;&#9;end)
&#9;btn3Connection = popup.Frame.Btn3.Clicked.Changed:connect(
&#9;&#9;function() 
&#9;&#9;&#9;if popup.Frame.Btn3.Clicked.Value then 
&#9;&#9;&#9;&#9;hidePopup()
&#9;&#9;&#9;&#9;if btn3Handler then btn3Handler() end
&#9;&#9;&#9;end
&#9;&#9;end)
&#9;-- Disconnect events when popup gets deleted
&#9;popup.Add.Disabled = false
&#9;popupConnection = popup.Add.Changed:connect(function() if popup.Add.Disabled then hidePopup() end end)
end
