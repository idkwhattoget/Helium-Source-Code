function getBaseplate()
&#9;local baseplate
&#9;if buildingArea:FindFirstChild(&quot;BasePlate&quot;) then
&#9;&#9;baseplate = buildingArea.BasePlate
&#9;elseif buildingArea:FindFirstChild(&quot;PlayerArea&quot;) then
&#9;&#9;if buildingArea.PlayerArea:FindFirstChild(&quot;BasePlate&quot;) then
&#9;&#9;&#9;baseplate = buildingArea.PlayerArea.BasePlate
&#9;&#9;else
&#9;&#9;&#9;return nil
&#9;&#9;end
&#9;end
