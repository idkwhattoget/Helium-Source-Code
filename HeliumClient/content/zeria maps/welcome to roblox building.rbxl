function waitForChild(instance, name)
&#9;while not instance:FindFirstChild(name) do
&#9;&#9;instance.ChildAdded:wait()
&#9;end
end
function waitForProperty(instance, name)
&#9;while not instance[name] do
&#9;&#9;instance.Changed:wait()
&#9;end
end
function waitForAnyChild(instance)
&#9;while not instance:GetChildren()[1] do
&#9;&#9;instance.ChildAdded:wait()
&#9;end
end

-- TWEENERocal tweener
function tween_init()
&#9;-- Tweener must be in PlayerGui.Gui.Tweener.  This script can be called from anywhere inside PlayerGui.
&#9;local playerGui = script.Parent
&#9;while(not playerGui:IsA(&quot;PlayerGui&quot;)) do playerGui = playerGui.Parent end
&#9;local tweenerParent = script.Parent.Parent.Parent -- Change this
&#9;waitForChild(playerGui, &quot;Gui&quot;)
&#9;waitForChild(playerGui.Gui, &quot;Tweener&quot;)
&#9;tweener = playerGui.Gui.Tweener&#9;
&#9;-- Add scripts you&apos;ll use here
&#9;waitForChild(tweener, &quot;Tweener_UDim2&quot;)
end
function tween(target, time, params)
&#9;--print(&quot;tween()&quot;, target.Name)
&#9;if not tweener then tween_init() end
&#9;local tween = tweener.Tweener_UDim2:Clone()
&#9;if time then tween.time.Value = time end
&#9;if(params.Position) then
&#9;&#9;-- animate position
&#9;&#9;tween.Position.Value = true
&#9;&#9;tween.Position.xScale.Value  = params.Position.X.Scale
&#9;&#9;tween.Position.xOffset.Value = params.Position.X.Offset
&#9;&#9;tween.Position.yScale.Value  = params.Position.Y.Scale
&#9;&#9;tween.Position.yOffset.Value = params.Position.Y.Offset
&#9;end
&#9;if(params.Size) then
&#9;&#9;tween.Size.Value = true
&#9;&#9;tween.Size.xScale.Value  = params.Size.X.Scale
&#9;&#9;tween.Size.xOffset.Value = params.Size.X.Offset
&#9;&#9;tween.Size.yScale.Value  = params.Size.Y.Scale
&#9;&#9;tween.Size.yOffset.Value = params.Size.Y.Offset
&#9;end
&#9;if params.easing then tween.easing.Value = params.easing&#9;end
&#9;if params.overwrite then tween.overwrite.Value = params.overwrite end
&#9;if params.transformAroundCenter then tween.transformAroundCenter.Value = params.transformAroundCenter end
&#9;if params.delay then tween.delay.Value = params.delay end
&#9;tween.Parent = target
&#9;tween.Disabled = false -- go go go!
end
function killTweens(a)
&#9;waitForChild(tweener, &quot;KillTweens&quot;)
&#9;waitForChild(tweener.KillTweens, &quot;target&quot;)
&#9;tweener.KillTweens.target.Value = a
&#9;tweener.KillTweens.Disabled = false
end
