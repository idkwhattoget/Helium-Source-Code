&#9;&#9;downY = base.Position.Y
&#9;&#9;upY = downY + buttonHeight
&#9;&#9;bodyPosition.position = Vector3.new(base.Position.X, upY, base.Position.Z)
&#9;&#9;bodyGyro_updateCFrame()
&#9;end
end

function bodyGyro_updateCFrame()
&#9;bodyGyro.cframe = base.CFrame
end

