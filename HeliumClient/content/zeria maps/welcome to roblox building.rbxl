end

function changeBaseplateColor()
&#9;currentColor = currentColor + 1
&#9;if currentColor &gt; #colors then
&#9;&#9;currentColor = 1
&#9;end
&#9;local baseplate = getBaseplate()
&#9;if baseplate then
&#9;&#9;baseplate.BrickColor = colors[currentColor]
&#9;&#9;offColor = colors[currentColor]
&#9;end
end

