function clearBaseplate(player)
&#9;-- If they don&apos;t have a player 
&#9;if not buildingArea:FindFirstChild(&quot;PlayerArea&quot;) then
&#9;&#9;return
&#9;end
&#9;-- If they have StamperTool equipped, then temporarily unequip it.
&#9;-- This is because when we clear the baseplate, we also clear their ghost part, which is dangerous and can break StampScript.
&#9;stamperToolWasEquipped = false
&#9;unequipStamperToop(player)  -- This can be run even if they don&apos;t have it equipped.
&#9;-- Get all parts
&#9;local parts  = buildingArea.PlayerArea:GetChildren()
&#9;-- Remove all parts except BasePlate and Player.
&#9;for i=1, #parts do
&#9;&#9;local type = parts[i].className
&#9;&#9;if     type == &quot;BoolValue&quot;&#9;&#9;&#9;then --
&#9;&#9;elseif type == &quot;BrickColorValue&quot;&#9;then --
&#9;&#9;elseif type == &quot;CFrameValue&quot;&#9;&#9;then --
&#9;&#9;elseif type == &quot;Color3Value&quot;&#9;&#9;then --
&#9;&#9;elseif type == &quot;IntValue&quot;      &#9;then --
&#9;&#9;elseif type == &quot;NumberValue&quot; &#9;&#9;then --
&#9;&#9;elseif type == &quot;ObjectValue&quot; &#9;&#9;then --
&#9;&#9;elseif type == &quot;StringValue&quot; &#9;&#9;then --
&#9;&#9;elseif type == &quot;Vector3Value&quot; &#9;then --
&#9;&#9;elseif parts[i].Name==&quot;BasePlate&quot; then -- 
&#9;&#9;else
&#9;&#9;&#9;parts[i]:Remove()
&#9;&#9;end
&#9;end
&#9;showPopup(player, &quot;notify&quot;, &quot;Baseplate Cleared&quot;, nil, &quot;OK&quot;)
&#9;-- Re-equip stamper tool after they click okay.
&#9;--showPopup(player, &quot;notify&quot;, &quot;Baseplate Cleared&quot;, nil, &quot;OK&quot;, function() wait(0.3) if stamperToolWasEquipped then equipStamperTool(player) end end)
end

function equipStamperTool(player)
&#9;waitForChild(player, &quot;Backpack&quot;)
&#9;if player.Backpack:FindFirstChild(&quot;StamperTool&quot;) then
&#9;&#9;player.Backpack.StamperTool.Parent = player.Character
&#9;end
end
function unequipStamperToop(player)
&#9;waitForChild(player, &quot;Backpack&quot;)
&#9;if player.Character:FindFirstChild(&quot;StamperTool&quot;) then
&#9;&#9;stamperToolWasEquipped = true
&#9;&#9;player.Character.StamperTool.Parent = player.Backpack
&#9;end
end

function showPopup1(player)
&#9;if not player then
&#9;&#9;return
&#9;end
&#9;waitForChild(buildingArea, &quot;Player&quot;)
&#9;-- Check if this player owns this baseplate
&#9;if(player.Name ~= buildingArea.Player.Value) then
&#9;&#9;local popupText = &quot;This area belongs to &quot; .. buildingArea.Player.Value .. &quot;.  You don&apos;t have permission.&quot;
&#9;&#9;if(buildingArea.Player.Value == &quot;&quot;) then
&#9;&#9;&#9;popupText = &quot;This area has no owner.&quot;
&#9;&#9;end
&#9;&#9;showPopup(player, &quot;error&quot;, &quot;Can&apos;t Clear This Area&quot;, popupText)
&#9;&#9;return
&#9;end
&#9;-- This player has permission
&#9;local title = &quot;Clear Your Baseplate?&quot;
&#9;local text = &quot;Clear all your parts from your baseplate?  This cannot be undone.&quot;
&#9;local btn1Text = &quot;Clear Baseplate&quot;
&#9;showPopup(player, &quot;confirm&quot;, title, text, btn1Text, function() clearBaseplate(player) end)
end

