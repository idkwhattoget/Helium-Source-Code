-- adds an edge going from edge[1] to edge[2] and back again
function addNeighbors(edge)
&#9;-- take care of net structure
&#9;if network[tostring(edge[1])] == nil then network[tostring(edge[1])] = {} costs[tostring(edge[1])] = {} end
&#9;if network[tostring(edge[2])] == nil then network[tostring(edge[2])] = {} costs[tostring(edge[2])] = {} end
&#9;table.insert(network[tostring(edge[1])], tostring(edge[2]))
&#9;table.insert(network[tostring(edge[2])], tostring(edge[1]))

&#9;-- add in costs of edges
&#9;local wayPoint1 = wayPoints:FindFirstChild(tostring(edge[1]))
&#9;local wayPoint2 = wayPoints:FindFirstChild(tostring(edge[2]))
&#9;if wayPoint2 == nil then print(tostring(edge[2])) end
&#9;--local edgeCost = math.sqrt((wayPoint1.Position - wayPoint2.Position):Dot(wayPoint1.Position - wayPoint2.Position))
